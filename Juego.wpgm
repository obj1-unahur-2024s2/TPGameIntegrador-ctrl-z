import wollok.game.*
import Jugador.*
program pruebas{
    game.width(10)
    game.height(10)
    game.cellSize(50)
    game.addVisualCharacter(katos)
    game.addVisual(llave)
    game.addVisual(enemigo)
    game.addVisual(espada)
    katos.tocarItem(espada)
    katos.tocarItem(llave)
    game.onCollideDo(enemigo, {katos => enemigo.quitarVida(katos.dano())})
    game.onCollideDo(enemigo, {katos => katos.perderVida(enemigo.dano())})
    game.title("katos")
    game.start()
    
}

