import wollok.game.*
import Jugador.*
program pruebas{
    game.width(10)
    game.height(10)
    game.cellSize(50)
    game.addVisualCharacter(katos)
    const llave = new Objetos (imagen= "llave.png" , position = game.center())
    game.addVisual(llave)
    game.title("katos")
    game.start()
    game.onCollideDo(llave, game.say(katos,"hola"))
}

